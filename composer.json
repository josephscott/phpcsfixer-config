{
	"name": "josephscott/phpcsfixer-config",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.1"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.52",
		"phpstan/phpstan": "^1.10",
		"pestphp/pest": "^2.34"
	},
	"autoload": {
		"classmap": [
			"src/josephscott/phpcsfixer-config.php"
		]
	},
	"config": {
		"classmap-authoritative": true,
		"optimize-autoloader": true,
		"allow-plugins": false
	}
}
